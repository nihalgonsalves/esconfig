{
  "name": "Node.js",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": ["esbenp.prettier-vscode", "dbaeumer.vscode-eslint"]
    }
  },
  "onCreateCommand": "npm config set -g '//registry.npmjs.org/:_authToken' \"${NPM_TOKEN}\"",
  "postCreateCommand": "yarn install",
  "remoteUser": "node"
}
